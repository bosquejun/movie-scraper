{
	"name": "api",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"dev": "nodemon --exec ts-node --require tsconfig-paths/register src/index.ts",
		"start": "cross-env NODE_PATH=dist node -r tsconfig-paths/register dist/index.js",
		"build": "tsc",
		"typecheck": "tsc --noEmit",
		"clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist && rm *.tsbuildinfo",
		"test": "jest --setupFiles dotenv/config",
		"lint": "eslint . --quiet",
		"db:make": "sequelize-cli migration:generate",
		"db:make-seed": "sequelize-cli seed:generate",
		"db:seed": "sequelize-cli db:seed",
		"db:seed:all": "sequelize-cli db:seed:all",
		"db:seed:undo": "sequelize-cli db:seed:undo",
		"db:migrate": "sequelize-cli db:migrate",
		"db:rollback": "sequelize-cli db:migrate:undo"
	},
	"devDependencies": {
		"@comeback/errors": "*",
		"@comeback/eslint-config": "*",
		"@comeback/logger": "*",
		"@comeback/model": "*",
		"@comeback/tsconfig": "*",
		"@comeback/utils": "*",
		"@types/bcryptjs": "^2.4.6",
		"@types/cors": "^2.8.17",
		"@types/express": "^4.17.21",
		"@types/node": "^22.5.0",
		"cross-env": "^7.0.3",
		"nodemon": "^3.1.4",
		"regenerator-runtime": "^0.14.1",
		"sequelize-cli": "^6.6.2",
		"ts-node": "^10.9.2",
		"ts-node-dev": "^2.0.0",
		"tsup": "^8.2.4",
		"typescript": "^5.5.4"
	},
	"dependencies": {
		"@comeback/express-lib": "*",
		"@comeback/redis": "*",
		"@comeback/schema": "*",
		"bcrypt": "^5.1.1",
		"bcryptjs": "^2.4.3",
		"cheerio": "^1.0.0",
		"cors": "^2.8.5",
		"dotenv": "^16.4.5",
		"express": "^4.19.2",
		"jsonwebtoken": "^9.0.2",
		"pg": "^8.12.0",
		"robots-parser": "^3.0.1",
		"tsconfig-paths": "^4.2.0"
	}
}
